{
  "name": "@vscode-use/utils",
  "version": "0.1.10",
  "packageManager": "pnpm@7.2.1",
  "description": "",
  "author": "Simon He",
  "license": "MIT",
  "funding": "https://github.com/sponsors/Simon-He95",
  "homepage": "https://github.com/vscode-use/utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vscode-use/utils.git"
  },
  "bugs": "https://github.com/vscode-use/utils/issues",
  "keywords": [
    "vscode-use",
    "@vscode-use/utils",
    "utils",
    "vscode"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.77.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "scripts": {
    "build": "tsup ./src/index.ts --format cjs,esm --dts --clean --minify",
    "dev": "tsup ./src/index.ts --format cjs,esm --dts --clean --watch src",
    "format": "prettier --write --cache .",
    "lint": "eslint . --cache",
    "lint:fix": "nr lint --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.15.0",
    "@antfu/ni": "^0.21.12",
    "@types/node": "^18.0.0",
    "@types/vscode": "^1.77.0",
    "bumpp": "^8.2.1",
    "eslint": "^9.0.0",
    "esno": "^0.16.3",
    "picocolors": "^1.0.0",
    "prettier": "^2.8.1",
    "tsup": "^8.1.0",
    "typescript": "^4.7.4",
    "vitest": "^0.15.1"
  },
  "lint-staged": {
    "*": [
      "prettier --write --cache --ignore-unknown"
    ],
    "*.{vue,js,ts,jsx,tsx,md,json}": "eslint --fix"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
